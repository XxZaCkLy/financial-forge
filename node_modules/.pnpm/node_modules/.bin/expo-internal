#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/cli/build/bin/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/cli/build/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/cli/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules:/workspaces/financial-forge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/cli/build/bin/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/cli/build/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/cli/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules/@expo/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+cli@54.0.8_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_a372fcb422c55d1009a2c39c628e7486/node_modules:/workspaces/financial-forge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@expo/cli/build/bin/cli" "$@"
else
  exec node  "$basedir/../@expo/cli/build/bin/cli" "$@"
fi
