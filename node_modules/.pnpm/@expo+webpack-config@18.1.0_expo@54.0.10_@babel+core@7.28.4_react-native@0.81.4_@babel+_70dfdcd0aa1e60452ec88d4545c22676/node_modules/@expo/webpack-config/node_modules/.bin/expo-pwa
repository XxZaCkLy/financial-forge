#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/financial-forge/node_modules/.pnpm/expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules/expo-pwa/build/node_modules:/workspaces/financial-forge/node_modules/.pnpm/expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules/expo-pwa/node_modules:/workspaces/financial-forge/node_modules/.pnpm/expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules:/workspaces/financial-forge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/financial-forge/node_modules/.pnpm/expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules/expo-pwa/build/node_modules:/workspaces/financial-forge/node_modules/.pnpm/expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules/expo-pwa/node_modules:/workspaces/financial-forge/node_modules/.pnpm/expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules:/workspaces/financial-forge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules/expo-pwa/build/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../expo-pwa@0.0.125_expo@54.0.10_@babel+core@7.28.4_react-native@0.81.4_@babel+core@7.28.4_2144c3354490901c706e8e9c935358c9/node_modules/expo-pwa/build/cli.js" "$@"
fi
