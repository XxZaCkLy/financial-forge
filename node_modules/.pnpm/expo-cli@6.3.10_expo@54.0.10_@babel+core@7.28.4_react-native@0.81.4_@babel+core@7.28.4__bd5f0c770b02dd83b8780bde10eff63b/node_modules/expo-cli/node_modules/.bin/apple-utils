#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/financial-forge/node_modules/.pnpm/@expo+apple-utils@1.0.0/node_modules/@expo/apple-utils/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+apple-utils@1.0.0/node_modules/@expo/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+apple-utils@1.0.0/node_modules:/workspaces/financial-forge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/financial-forge/node_modules/.pnpm/@expo+apple-utils@1.0.0/node_modules/@expo/apple-utils/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+apple-utils@1.0.0/node_modules/@expo/node_modules:/workspaces/financial-forge/node_modules/.pnpm/@expo+apple-utils@1.0.0/node_modules:/workspaces/financial-forge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@expo+apple-utils@1.0.0/node_modules/@expo/apple-utils/bin.js" "$@"
else
  exec node  "$basedir/../../../../../@expo+apple-utils@1.0.0/node_modules/@expo/apple-utils/bin.js" "$@"
fi
